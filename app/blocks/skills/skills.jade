mixin skills(skills)
	+b.skills&attributes(attributes)
		each skill__list, skill__title in skills
			section.m-skills
				h4.m-title.m-title_4 #{skill__title}
				ul.m-skills__list
					each level, skill__title in skill__list
						li.m-skills__item
							span.m-skills__title #{skill__title}
							svg(viewBox="0 0 110 110").m-graph
								circle(cx="55" cy="55" r="45").m-graph__canvas
								circle(cx="55" cy="55" r="45" class='m-graph__progress m-graph__progress_' + level)
